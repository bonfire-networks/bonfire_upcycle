<div class="flex flex-col w-full space-y-2">
  {#for intent <- @intents}
    <Bonfire.UI.Upcycle.IntentLive
      key={e(intent, :id)}
      name={e(intent, :name, nil)}
      note={e(intent, :note, nil)}
      intent={intent}
    />
  {#else}
    <Bonfire.UI.Common.EmptyFeed
      feedback_title={l("No item were found")}
    />
  {/for}
</div>
